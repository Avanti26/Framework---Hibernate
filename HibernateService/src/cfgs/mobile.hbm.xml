<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

  	<hibernate-mapping>
   	<class name="domain.Mobile" table="mobile_master">
   		<id name="mobileId" column="mobile_id"></id>
   		<property name="mobileName" column="mobile_name"/>
   		<property name="mobileQty" column="mobile_qty"/>
   		<property name="mobilePrice" column="mobile_price"/>
   		<property name="mobileCategory" column="mobile_category"/>
   	</class>
	</hibernate-mapping>